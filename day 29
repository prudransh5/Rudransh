#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int A, B, C, X;
        scanf("%d %d %d %d", &A, &B, &C, &X);

        if ((A + B >= X) || (B + C >= X) || (A + C >= X)) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
